{
  "name": "michaelhall/rss-feed",
  "description": "Simple RSS feed creator for PHP",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "rss",
    "feed"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=8.0",
    "datatypes/datatypes": "^2.4",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "MichaelHall\\RssFeed\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelHall\\RssFeed\\Tests\\": "tests"
    }
  }
}
